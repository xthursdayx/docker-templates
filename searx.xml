<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/xthursdayx/docker-templates/master/searx.xml</TemplateURL>
  <Name>searx</Name>
  <Description>
    A privacy-respecting, hackable metasearch engine.
  </Description>
  <Overview>
    A privacy-respecting, hackable metasearch engine.
  </Overview>
  <Registry>https://registry.hub.docker.com/u/xthursdayx/searx/</Registry>
  <GitHub>https://github.com/xthursdayx/searx</GitHub>
  <Repository>xthursdayx/searx</Repository>
  <Project>https://github.com/asciimoo/searx</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8888</HostPort>
        <ContainerPort>8888</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
    <Variable>
      <Name>BASE_URL</Name>
      <Value>https://searx.domain.tld</Value>
    </Varibale>
    <Variable>
      <Name>IMAGE_PROXY</Name>
      <Value>True</Value>
    </Varibale>
  </Environment>
  <WebUI>http://[IP]:[PORT:8888]/</WebUI>
  <Banner></Banner>
  <Icon>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/images/nzbhydra2-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Containers>